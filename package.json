    "@capacitor/filesystem": "^8.1.0",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/share": "^8.0.0",
    "@capacitor/splash-screen": "latest",
    "html2canvas": "^1.4.1",
    "html5-qrcode": "^2.3.8",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.4",
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "latest",
    "@types/qrcode": "^1.5.6",
