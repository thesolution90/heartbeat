{
  "name": "heartbeat",
  "version": "1.0.0",
  "description": "Guess your favourite songs",
  "type": "module",
  "keywords": [
    "capacitor",
    "mobile"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "android": "npm run build && npx cap sync && npx cap open android",
